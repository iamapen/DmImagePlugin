<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <title>iamapen/dmimage-plugin</title>
  <parser>
    <target>_build/phpdoc</target>
    <encoding>utf8</encoding>
    <markers>
      <item>TODO</item>
      <item>FIXME</item>
      <item>XXX</item>
    </markers>
    <extensions>
      <extension>php</extension>
    </extensions>
    <visibility></visibility>
  </parser>
  <transformer>
    <target>_build/phpdoc</target>
  </transformer>
  <logging>
    <level>warn</level>
    <paths>
      <default>_build/log/phpdoc/{DATE}.log</default>
      <errors>_build/log/phpdoc/{DATE}_errors.log</errors>
    </paths>
  </logging>
  <transformations>
    <template name="responsive-twig" />
<!--
    <template name="clean" />
    <template name="new-black" />
    <template name="old-ocean" />
    <template name="responsive" />
    <template name="responsive-twig" />
    <template name="zend" />
-->
  </transformations>

  <files>
    <directory>src</directory>
    <ignore>/unittest/*</ignore>
  </files>
</phpdoc>
